{
  "rewrites": [
    {
      "source": "/preview/:slug",
      "destination": "/preview.html"
    },
    {
      "source": "/watch/:slug",
      "destination": "/watch.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    },
    {
      "source": "/(.*).html",
      "headers": [
        {
          "key": "Cache-Control", 
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}